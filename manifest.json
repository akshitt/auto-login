{
  "manifest_version": 2,

  "name": "Auto-login",
  "description": "This extension will login to internet.iitb.in directly",
  "version": "1.0",

  "browser_action": {
   "default_icon": "iitb.png"
  },
  "background":
  {
     "scripts": ["popup.js"]
  },
  "permissions": [ "activeTab","https://internet.iitb.ac.in/*"]
}